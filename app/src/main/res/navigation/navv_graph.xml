<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navv_graph"
    app:startDestination="@id/record_Fragment">

    <fragment
        android:id="@+id/record_Fragment"
        android:name="com.example.voicerecordingapp.Record_Fragment"
        android:label="fragment_record_"
        tools:layout="@layout/fragment_record_" >
        <action
            android:id="@+id/action_record_Fragment_to_audio_list_F2"
            app:destination="@id/audio_list_F2"
            app:exitAnim="@anim/fadee_out"
            app:enterAnim="@anim/slide_in"
            app:popExitAnim="@anim/fadee_out"
            app:popEnterAnim="@anim/slideout"

            />
    </fragment>
    <fragment
        android:id="@+id/audio_list_F2"
        android:name="com.example.voicerecordingapp.Audio_list_F"
        android:label="fragment_audio_list_"
        tools:layout="@layout/fragment_audio_list_" />
</navigation>